                                    "address": {
                                        "type": "string",
                                        "example": "123 Main St"
                                    },
                                    "city": {
                                        "type": "string",
                                        "example": "Anytown"
                                    },
                                    "country": {
                                        "type": "string",
                                        "example": "US"
                                    },
                                    "postal_code": {
                                        "type": "string",
                                        "example": "12345"
